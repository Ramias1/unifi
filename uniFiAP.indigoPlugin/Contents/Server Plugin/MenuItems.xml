  <MenuItem id="dGR"> <Name>-------------Groups   ----------</Name><CallbackMethod>inpDummy</CallbackMethod></MenuItem>

    <MenuItem id="addToGroups">
        <Name>Add or remove device types from to groups</Name>
        <ConfigUI>
            <Field     id="addRemoveGroupsWhichDevice" type="menu" defaultValue="system">
                <Label>select which type of device:</Label>
                 <List>
                    <Option value="system"    >system devices (AP, switch etc)</Option>
                    <Option value="neighbor"  >neighbor wifi routers etc </Option>
                    <Option value="wifi"      >regular devices connected though wifi </Option>
                    <Option value="LAN"       >regular devices connected though LAN  (not Wifi) </Option>
                </List>
            </Field>
            <Field     id="addRemoveGroupsWhichGroup" type="menu" defaultValue="0">
                <Label>select which Group you want to add to / remove from:</Label>
                 <List>
                    <Option value="Group0"    >group0</Option>
                    <Option value="Group1"    >group1</Option>
                    <Option value="Group2"    >group2</Option>
                    <Option value="Group3"    >group3</Option>
                    <Option value="Group4"    >group4</Option>
                    <Option value="Group5"    >group5</Option>
                    <Option value="Group6"    >group6</Option>
                    <Option value="Group7"    >group7</Option>
                    <Option value="Group8"    >group8</Option>
                    <Option value="Group9"    >group9</Option>
                </List>
            </Field>
            <Field    id="confirmadd" type="button" tooltip="" >
                <Label>Click to add</Label><Title>CONFIRM</Title> <CallbackMethod>buttonConfirmAddDevGroupCALLBACK</CallbackMethod>
            </Field>
            <Field    id="confirmrem" type="button" tooltip="" >
                <Label>Click to remove</Label><Title>CONFIRM</Title> <CallbackMethod>buttonConfirmRemDevGroupCALLBACK</CallbackMethod>
            </Field>
        </ConfigUI>
    </MenuItem>


    <MenuItem id="dAC"> <Name>-------------Actions   ----------</Name><CallbackMethod>inpDummy</CallbackMethod></MenuItem>
    <MenuItem id="setDevicesOptimized">
        <Name>Set all UniFi devices props to .... </Name>
        <ConfigUI>
            <Field    id="confirm11" type="button" tooltip="will set all WiFi devices to Optimized, IDLETIME = 30 secs or Uptime changed" >
                <Label>Change all Wifi devices to OPTIMIZED</Label><Title>CONFIRM</Title> <CallbackMethod>buttonConfirmSetWifiOptCALLBACK</CallbackMethod>
            </Field>
            <Field    id="confirm12" type="button" tooltip="will set all WiFi devices to use IDLETIME , ideltime = 30 secs" >
                <Label>Change all Wifi devices to IDLE time</Label><Title>CONFIRM</Title> <CallbackMethod>buttonConfirmSetWifiIdleCALLBACK</CallbackMethod>
            </Field>
            <Field    id="confirm13" type="button" tooltip="will set all WiFi devices to use Uptime must change" >
                <Label>Change all Wifi devices to UpTime changed</Label><Title>CONFIRM</Title> <CallbackMethod>buttonConfirmSetWifiUptimeCALLBACK</CallbackMethod>
            </Field>

            <Field id="simpleSeparator1" type="separator"/>

            <Field    id="confirm3" type="button" tooltip="will set all non-WiFi devices to Opt DHCP AND SWITCH with DHCP age lt 60 secs OR SWITCH UPTIME must change" >
                <Label>Change all Non-Wifi devices to OPTIMIZED  </Label><Title>CONFIRM</Title> <CallbackMethod>buttonConfirmSetNonWifiOptCALLBACK</CallbackMethod>
            </Field>
            <Field    id="confirm4" type="button" tooltip="will set all non-WiFi devices to  SWITCH with SWITCH UPTIME must change" >
                <Label>Change all Non-Wifi devices to SWITCH</Label><Title>CONFIRM</Title> <CallbackMethod>buttonConfirmSetNonWifiToSwitchCALLBACK</CallbackMethod>
            </Field>
            <Field    id="confirm5" type="button" tooltip="will set all non-WiFi devices to  DHCP with DHCP AGE lt 60 secs" >
                <Label>Change all Non-Wifi devices to DHCP</Label><Title>CONFIRM</Title> <CallbackMethod>buttonConfirmSetNonWifiToDHCPCALLBACK</CallbackMethod>
            </Field>

            <Field id="simpleSeparator2" type="separator"/>

            <Field    id="confirm6" type="button" tooltip="" >
                <Label>Change all devices to USE PING for UP test</Label><Title>CONFIRM</Title> <CallbackMethod>buttonConfirmSetUsePingUPonCALLBACK</CallbackMethod>
            </Field>
            <Field    id="confirm7" type="button" tooltip="" >
                <Label>Change all devices to NOT use PING for UP test</Label><Title>CONFIRM</Title> <CallbackMethod>buttonConfirmSetUsePingUPoffCALLBACK</CallbackMethod>
            </Field>

            <Field    id="confirm8" type="button" tooltip="" >
                <Label>Change all devices to USE PING for DOWN test</Label><Title>CONFIRM</Title> <CallbackMethod>buttonConfirmSetUsePingDOWNonCALLBACK</CallbackMethod>
            </Field>
            <Field    id="confirm9" type="button" tooltip="" >
                <Label>Change all devices to NOT use PING for DOWN test</Label><Title>CONFIRM</Title> <CallbackMethod>buttonConfirmSetUsePingDOWNoffCALLBACK</CallbackMethod>
            </Field>

            <Field id="simpleSeparator10" type="separator"/>

            <Field    id="expirationTime"       type="menu"  defaultValue="90" tooltip="">
                <Label>Set time for UP to DOWN to EXPIRED for ALL devices</Label>
                <List>
                    <Option value="1"    > 1  seconds (for testing)</Option>
                    <Option value="10"   >10  seconds (for testing)</Option>
                    <Option value="20"   >20  seconds (for testing)</Option>
                    <Option value="30"   >30  seconds </Option>
                    <Option value="40"   >40  seconds </Option>
                    <Option value="60"   >60  seconds </Option>
                    <Option value="75"   >75 seconds</Option>
                    <Option value="90"   >90 seconds</Option>
                    <Option value="100"  >100 seconds</Option>
                    <Option value="120"  >120 seconds</Option>
                    <Option value="150"  >150 seconds</Option>
                    <Option value="180"  > 3  minutes</Option>
                    <Option value="240"  > 4  minutes</Option>
                    <Option value="300"  > 5  minutes</Option>
                    <Option value="360"  > 6  minutes</Option>
                    <Option value="420"  > 7  minutes</Option>
                    <Option value="490"  > 8  minutes</Option>
                    <Option value="540"  > 9  minutes</Option>
                    <Option value="600"  >10  minutes</Option>
                    <Option value="-1"   >use common expiration time</Option>
                </List>
            </Field>
            <Field    id="confirm10" type="button" tooltip="" >
                <Label>Change all devices to this exp time</Label><Title>CONFIRM</Title> <CallbackMethod>buttonConfirmSetExpTimeCALLBACK</CallbackMethod>
            </Field>


        </ConfigUI>
    </MenuItem>

