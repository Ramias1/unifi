    <MenuItem id="printDevInfoFromController">
        <Name>Print info from UniFi Controller DB</Name>
        <ConfigUI>
            <Field  id="Label01" type="label"> 
            <Label>Make sure that retention is set properly in MAINTENANCE in the controller
            and enable "use controller info mut NOT be off  </Label> </Field>
            <Field id="simpleSeparator0" type="separator"/>

            <Field  id="Label1" type="label"> <Label>Print info for system devices:</Label> </Field>
            <Field     id="MACdeviceSelectedsys" type="menu" defaultValue="">
                <Label>select which MAC # / device:</Label>
                <List class="self" filter="" method="filterUNIFIsystemDevice"  dynamicReload="false"/>
            </Field>
            <Field    id="confirm1" type="button" tooltip="" >
                <Label>Click to  </Label><Title>PRINT</Title> <CallbackMethod>buttonConfirmPrintDevInfoFromControllerCALLBACK</CallbackMethod>
            </Field>
            <Field id="simpleSeparator1" type="separator"/>

            <Field  id="Label2" type="label"> <Label>Print info for CLIENT devices:</Label> </Field>
            <Field     id="MACdeviceSelectedclient" type="menu" defaultValue="">
                <Label>select which MAC # / device:</Label>
                <List class="self" filter="" method="filterMACunifiOnlyUP"  dynamicReload="false"/>
            </Field>
            <Field    id="confirm2" type="button" tooltip="" >
                <Label>Click to </Label><Title>PRINT</Title> <CallbackMethod>buttonConfirmPrintClientInfoFromControllerCALLBACK</CallbackMethod>
            </Field>

            <Field  id="Label21" type="label"> 
<Label>                Does not work for type clients, ie gw, ap, sw
                       and long time expired devices = not in controller memory</Label> </Field>
            <Field id="simpleSeparator2" type="separator"/>

            <Field  id="Label3" type="label"> <Label>Print HEALTH info:</Label> </Field>
            <Field    id="confirm3" type="button" tooltip="" >
                <Label>Click to </Label><Title>PRINT</Title> <CallbackMethod>buttonConfirmPrintHealthInfoFromControllerCALLBACK</CallbackMethod>
            </Field>
            <Field id="simpleSeparator3" type="separator"/>

            <Field  id="Label4" type="label"> <Label>Print PortForward info:</Label> </Field>
            <Field    id="confirm4" type="button" tooltip="" >
                <Label>Click to </Label><Title>PRINT</Title> <CallbackMethod>buttonConfirmPrintPortForWardInfoFromControllerCALLBACK</CallbackMethod>
            </Field>
            <Field id="simpleSeparator4" type="separator"/>

            <Field  id="Label5" type="label"> <Label>Print user info:</Label> </Field>
            <Field    id="confirm5" type="button" tooltip="active users / clients" >
                <Label>Click to </Label><Title>PRINT</Title> <CallbackMethod>buttonConfirmPrintuserInfoFromControllerCALLBACK</CallbackMethod>
            </Field>
            <Field id="simpleSeparator5" type="separator"/>

            <Field  id="Label51" type="label"> <Label>Print all user info:</Label> </Field>
            <Field    id="confirm51" type="button" tooltip="includes expired users / clients" >
                <Label>Click to </Label><Title>PRINT</Title> <CallbackMethod>buttonConfirmPrintalluserInfoFromControllerCALLBACK</CallbackMethod>
            </Field>
            <Field id="simpleSeparator51" type="separator"/>

            <Field  id="Label6" type="label"> <Label>Print Alarm info:</Label> </Field>
            <Field    id="confirm6" type="button" tooltip="" >
                <Label>Click to </Label><Title>PRINT</Title> <CallbackMethod>buttonConfirmPrintAlarmInfoFromControllerCALLBACK</CallbackMethod>
            </Field>
            <Field id="simpleSeparator6" type="separator"/>

            <Field  id="Label7" type="label"> <Label>Print Events  (limited by retention set in controller):</Label> </Field>
            <Field     id="PrintEventInfoMaxEvents" type="menu" defaultValue="10">
                <Label>Maximum Number of events:</Label>
                <List>
                    <Option value="1"        >1</Option>
                    <Option value="2"        >2</Option>
                    <Option value="5"        >5</Option>
                    <Option value="10"       >10</Option>
                    <Option value="20"       >20</Option>
                    <Option value="100"      >100</Option>
                    <Option value="200"      >200</Option>
                    <Option value="500"      >500</Option>
                    <Option value="1000"     >1000</Option>
                    <Option value="2000"     >2000</Option>
                    <Option value="5000"     >5000</Option>
                    <Option value="999999999">all</Option>
                </List>
            </Field>
            <Field    id="PrintEventInfoLoginEvents" type="checkbox"  defaultValue="false"  > <Label>.. print LOGIN events:</Label>  </Field>
            
            <Field    id="confirm7" type="button" tooltip="" >
                <Label>Click to </Label><Title>PRINT</Title> <CallbackMethod>buttonConfirmPrintEventInfoFromControllerCALLBACK</CallbackMethod>
            </Field>
            <Field id="simpleSeparator7" type="separator"/>

            <Field  id="Label8" type="label"> <Label>Print 7 day stats:</Label> </Field>
            <Field    id="confirm8" type="button" tooltip="" >
                <Label>Click to </Label><Title>PRINT</Title> <CallbackMethod>buttonConfirmPrint7DayInfoFromControllerCALLBACK</CallbackMethod>
            </Field>
            <Field id="simpleSeparator8" type="separator"/>

            <Field  id="Label9" type="label"> <Label>Print 48 hour stats:</Label> </Field>
            <Field    id="confirm9" type="button" tooltip="" >
                <Label>Click to </Label><Title>PRINT</Title> <CallbackMethod>buttonConfirmPrint48HourInfoFromControllerCALLBACK</CallbackMethod>
            </Field>
            <Field id="simpleSeparator9" type="separator"/>

            <Field  id="Label91" type="label"> <Label>Print 5 Minutes stats:</Label> </Field>
            <Field    id="confirm91" type="button" tooltip="" >
                <Label>Click to </Label><Title>PRINT</Title> <CallbackMethod>buttonConfirmPrint5MinutesInfoFromControllerCALLBACK</CallbackMethod>
            </Field>
            <Field id="simpleSeparator91" type="separator"/>
            
            <Field  id="Label10" type="label"> <Label>Print W-lan Config (passwords will be printed!!):</Label> </Field>
            <Field    id="confirm10" type="button" tooltip="" >
                <Label>Click to </Label><Title>PRINT</Title> <CallbackMethod>buttonConfirmPrintWlanConfInfoFromControllerCALLBACK</CallbackMethod>
            </Field>

        </ConfigUI>
